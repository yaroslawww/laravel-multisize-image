{
  "name": "yaroslawww/laravel-multisize-image",
  "description": "Crop multiple sizes image while downloading",
  "keywords": [
    "laravel",
    "multisize",
    "laravel-multisize-image"
  ],
  "homepage": "https://github.com/yaroslawww/laravel-multisize-image",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaroslav Georgitsa",
      "email": "yaroslav.georgitsa@gmail.com",
      "homepage": "https://yaroslawww.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.8.15|^6.0|^7.0",
    "illuminate/http": "^5.8.15|^6.0|^7.0",
    "intervention/image": "^2.5"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "php-mock/php-mock-phpunit": "^2.5",
    "phpunit/phpunit": "^8.2",
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Gcsc\\LaravelMultisizeImage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gcsc\\LaravelMultisizeImage\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  }
}
